<?xml version='1.0' encoding='utf-8' ?>
<Config>
	<!--玩家的显示属性-->
	<PlayerData>
		<Data Name="id"/>
		<Data Name="name"/>
		<Data Name="icon"/>
    	<Data Name="iconbox"/>
   	 	<Data Name="sex"/>
	</PlayerData>

	<!-- 排行榜列表 -->
	<!-- Id 排行榜id -->
	<!-- ParentData 排行榜属性的父属性 -->
	<!-- RankData 排行榜属性 -->
	<!-- ShowData 额外的显示属性 -->
	<!-- RefreshType 刷新类型 0 间隔时间 1 每小时  2 每天 3 每周 4 每月 -->
	<!-- RefreshTime 刷新时间 时间间隔 : 单位秒  其他都是定点时间-->
	<!-- RefreshHour 刷新小时 -->
	<!-- RefreshMinute 刷新分钟 -->
	<!-- ZoneType 1 全区全服排行榜  2 分区排行榜 -->
	<!-- MaxCount 排行榜最大数量 -->

	<Ranks>
		<!-- 总评分排行榜 -->
		<Rank Id="1" ShardId="92001" ZoneType="1" ParentData="totalscore" RankData="score" MaxCount="100" RefreshType="0" RefreshTime="300" RefreshHour="0" RefreshMinute="0" Reset="0">
			<ShowData Name="score"/>
			<ShowData Name="winrate"/>
			<ShowData Name="topfive"/>
		</Rank>

		<!-- 总胜场排行榜 -->
